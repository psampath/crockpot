<div class="row">
    <div class="box">
      <% @recipes.each do |recipe| %>
      <div class="col-lg-12">
        <div class="well">
            <strong><%= link_to recipe.name, edit_recipe_path(recipe) %></strong>
            <p>
              <%= recipe.summary %>
            </p>
        </div>
      </div>
      <%end%>
    </div>
</div>
